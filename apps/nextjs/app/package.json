{
  "name": "nextjs",
  "version": "0.0.0",
  "engines": {
    "node": ">=12.13.1",
    "npm": ">=6.14.8"
  },
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "next",
    "build": "next build",
    "chromatic": "CHROMATIC_PROJECT_TOKEN=$(grep CHROMATIC_PROJECT_TOKEN .env.local | cut -d '=' -f2) && npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook",
    "storybook:publish": "npm run storybook:publish:local",
    "storybook:publish:local": "npm run chromatic",
    "type-check": "tsc",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "test:coverage:view": "jest --coverage && open coverage/lcov-report/index.html",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.2.3",
    "next": "^12.3.1",
    "next-compose-plugins": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-controls": "^6.1.9",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.5.12",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/storybook__addon-knobs": "^5.2.1",
    "babel-loader": "^8.2.1",
    "babel-preset-react-app": "^10.0.0",
    "chromatic": "^5.4.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.9.0",
    "typescript": "^4.0.5"
  },
  "license": "MIT"
}
